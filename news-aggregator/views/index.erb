<h1>All Articles</h1>
<br>
<table>
  <tr>
    <th>Article Title</th>
    <th>Description</th>
    <th>URL</th>
  </tr>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article[0] %></td>
      <td><%= article[1] %></td>
      <td>
        <a href= "http://<%= article[2] %>" ><%= article[2] %></a>
      </td>
    </tr>
  <% end %>
</table>
